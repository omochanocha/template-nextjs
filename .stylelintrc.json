{
  "extends": ["stylelint-config-standard", "stylelint-config-recess-order"],
  "ignoreFiles": ["**/node_modules/**"],
  "plugins": ["stylelint-order"],
  "rules": {
    "at-rule-no-deprecated": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind", "apply", "variants", "responsive", "screen", "theme"]
      }
    ]
  }
}
